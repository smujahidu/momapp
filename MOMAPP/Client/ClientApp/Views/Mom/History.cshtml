@{
    ViewBag.Title = "History";
    Layout = "~/Views/Shared/UserLayout.cshtml";
}


<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title></title>
    <link href="~/Scripts/bootstrap.css" rel="stylesheet" />
    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <link href="http://netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css" rel="stylesheet">

    <link href="http://netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">

    <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.11.1/jquery.min.js"></script>

    <script src="http://netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>


    <link rel="stylesheet" href="//code.jquery.com/ui/1.11.4/themes/smoothness/jquery-ui.css">
    <script src="//code.jquery.com/jquery-1.10.2.js"></script>
    <script src="//code.jquery.com/ui/1.11.4/jquery-ui.js"></script>

    <link href="~/Scripts/OurScripts/editor.css" rel="stylesheet" />

    <script src="~/Scripts/OurScripts/editor.js"></script>



    <script type="text/javascript">
        $(document).ready(function () {

            $("#description").Editor()
            $("#datepicker").datepicker();

            $.ajax({
                type: "POST",
                url: "http://localhost:50933/api/Mom/Getproject",
                crossDomain: true,
                headers: {
                    'Accept': 'application/json',
                    'Content-Type': 'text/plain'
                },
                //contentType: "application/json; charset=utf-8",
                dataType: 'json',

                success: function (data) {
                    var result = data;
                    // alert(result.length);
                    $("#ddlProject").append(
                           $('<option/>', {
                               value: 0,
                               text: "select"
                           })
                       );
                    for (var i = 0; i < data.length; i++) {
                        $("#ddlProject").append(
                            $('<option/>', {
                                value: data[i].ProjectID,
                                text: data[i].ProjectName
                            })
                        );
                    }
                }
            });
            })



    </script>

</head>
<body>
    @*@using (Html.BeginForm("CreateDoc", "Mom", FormMethod.Post))*@

    <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 well well-sm">
        <div class="row">
            <div class="col-lg-8">
                <div style="text-align:center">
                    <label for="comment">Select Project:</label>
                    <select id="ddlProject"></select><br />
                    <label for="comment" id="Subject">Select Date:</label>
                    <input type="text" id="datepicker"><br />
                </div><br />
                <div style="text-align:center">
                    <input type="submit" id="btn" class="btn btn-info" value=" Search ">
                </div>
            </div>
      </div>
   </div>

                <div class="col-xs-12 col-sm-12 col-md-12 col-lg-12 well well-sm" id="divMom">
                    <div class="row">
                        <div class="col-lg-8">
                            <label for="comment" id="Subject">Subject:</label></br>
                            <input class="form-control" name="UserName" id="sub" placeholder="Subject" type="text" />
                            <label for="comment" id="Body">Description:</label></br>
                            <textarea class="form-control" rows="5" id="description"></textarea>
                        </div>

                        <div class="col-lg-4" style="text-align:center" id="divresult">
                            @*<label for="comment">Employee List:</label></br>*@
                            <br /><br />
                            <label for="comment">Employee List:</label></br>
                            <select name="List" id="List" multiple="multiple" class="form-control"></select>
                            <table id="tbl"></table>
                        </div>
                    </div><br />
                    
                </div>



</body>
</html>


